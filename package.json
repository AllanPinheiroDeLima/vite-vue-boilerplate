{
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": ["vue3", "vue", "typescript", "vitest", "storybook", "pinia", "vue-router", "boilerplate"],
  "scripts": {
    "dev": "vite",
